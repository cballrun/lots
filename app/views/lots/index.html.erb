<h1>Lots</h1>

<% @lots.most_recent.each_with_index do |lot, index| %>
  <div id="lot-<%= index %>" >
  <h2> <%= lot.name %> </h2>
  <p> <%= lot.created_at %> </p>
  <h3> <%= link_to "Edit #{lot.name}", "lots/#{lot.id}/edit", method: :get %>   <%= link_to "Delete #{lot.name}", "lots/#{lot.id}", method: :delete %> </h3>
<% end %>

<h3> <%= link_to 'New Lot', '/lots/new' %> </h3>
 