<h1>Lots</h1>

<% @lots.most_recent.each_with_index do |lot, index| %>
  <div id="lot-<%= index %>" >
  <p> <%= lot.name %> </p>
  <p> <%= lot.created_at %> </p>
  <p> <%= link_to "Edit #{lot.name}", "lots/#{lot.id}/edit", method: :get %>
<% end %>

<h3> <%= link_to 'New Lot', '/lots/new' %> </h3>
 